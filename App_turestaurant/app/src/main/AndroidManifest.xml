<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sourcey.turestaurante">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.sourcey.turestaurante.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.sourcey.turestaurante.LoginActivity"
            android:theme="@style/AppTheme.Dark">
        </activity>

        <activity
            android:name="com.sourcey.turestaurante.SignupActivity"
            android:theme="@style/AppTheme.Dark">
        </activity>

        <activity
            android:name="com.sourcey.turestaurante.RestaurantActivity"
            android:theme="@style/AppTheme.Dark">
        </activity>

    </application>

</manifest>